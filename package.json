{
  "name": "jl-comerciales_core",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate:schema": "ts-node prisma/scripts/mergeSchemas.js",
    "prisma:db:push": "npm run prisma:generate:schema && prisma db push",
    "prisma:migrate": "npm run prisma:generate:schema && prisma migrate dev",
    "prisma:generate": "npm run prisma:generate:schema && prisma generate",
    "prisma:studio": "npm run prisma:generate:schema && prisma studio",
    "prisma:validate": "npm run prisma:generate:schema && prisma validate",
    "prisma:format": "npm run prisma:generate:schema && prisma format",
    "prisma:reset": "npm run prisma:generate:schema && prisma migrate reset",
    "prisma:deploy": "npm run prisma:generate:schema && prisma migrate deploy",
    "prisma:status": "npm run prisma:generate:schema && prisma migrate status",
    "clean": "rm -rf dist"
  },
  "keywords": [],
